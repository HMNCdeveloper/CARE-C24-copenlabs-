<controls:MetroWindow
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:xctk="http://schemas.xceed.com/wpf/xaml/toolkit"
        xmlns:controls="http://metro.mahapps.com/winfx/xaml/controls"
        xmlns:iconPacks="http://metro.mahapps.com/winfx/xaml/iconpacks"
        xmlns:wag="http://wpfanimatedgif.codeplex.com"
        xmlns:fa="http://schemas.fontawesome.io/icons/"
        xmlns:local="clr-namespace:HS5.Resources.Idiomas"       
        xmlns:av="http://schemas.microsoft.com/expression/blend/2008" xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" mc:Ignorable="av" x:Name="window" x:Class="MahAppsExample.MainWindow"       
        GlowBrush="{DynamicResource BlackBrush}"
        Icon="Resources/CopenIcon.ico"
        Title="" WindowState="Maximized" Height="487"  WindowStartupLocation="CenterScreen"  Background="White"  Closing="window_Closing" Closed="window_Closed">

    <Window.Resources>
        <SolidColorBrush x:Key="background" Color="#F5FEFF" />
        <SolidColorBrush x:Key="boxBack"  Color="White"/>
        <SolidColorBrush x:Key="section" Color="#FF3CB3DF"/>
        <SolidColorBrush x:Key="btn" Color="#FF3CB3DF"/>
        <SolidColorBrush x:Key="iconColor" Color="#01997E"/>
        <SolidColorBrush x:Key="borderSection" Color="#5A5C5D"/>
        <SolidColorBrush x:Key="letra" Color="Black"/>
        <SolidColorBrush x:Key="TabColor" Color="#FFF7F5F5"/>
        <SolidColorBrush x:Key="TabColorSelec" Color="#307F"/>
        <SolidColorBrush x:Key="Boton" Color="#0A3D6E"/>
        <SolidColorBrush x:Key="Boton2" Color="#2ecc71"/>
        <SolidColorBrush x:Key="letraBoton" Color="#FFF"/>

        <Style TargetType="RadioButton">
            <Setter Property="Margin" Value="10"/>
            <Setter Property="Background" Value="White"/>
            <Style.Triggers>
                <!-- Cambia el color de fondo cuando está marcado -->
                <Trigger Property="IsChecked" Value="True">
                    <Setter Property="Background" Value="#FF3CB3DF"/>
                </Trigger>
            </Style.Triggers>
        </Style>



        <!-- Define un estilo para los ListBoxItems -->
        <Style TargetType="ListBoxItem">
            <Setter Property="Padding" Value="3.5"/>
            <Style.Triggers>
                <!-- Cambia el color de fondo cuando se selecciona el ListBoxItem -->
                <Trigger Property="IsSelected" Value="True">
                    <Setter Property="Background" Value="#FF3CB3DF"/>
                    <Setter Property="Foreground" Value="Black"/>
                    <!-- Cambia el color del texto si es necesario -->
                </Trigger>
            </Style.Triggers>
        </Style>


        <!-- Estilo para el Header del TabItem -->
        <Style TargetType="TabControl" x:Key="CustomTabItemStyle">
            <Setter Property="Template">
                <Setter.Value>
                    <ControlTemplate TargetType="TabControl">
                        <Grid>
                            <Grid.ColumnDefinitions>
                                <!-- Definir columna para la barra lateral -->
                                <ColumnDefinition Width="Auto"/>
                                <!-- Definir columna para el contenido -->
                                <ColumnDefinition Width="*"/>
                            </Grid.ColumnDefinitions>
                            <!-- Sidebar con color azul -->
                            <Border Background="{StaticResource TabColor}" Grid.Column="0">
                                <!-- Contenedor para los TabItems -->
                                <TabPanel x:Name="HeaderPanel" Grid.Column="0" IsItemsHost="True"
                                  Margin="2,2,2,2"
                                  KeyboardNavigation.TabIndex="1"/>
                            </Border>
                            <!-- Contenido de los TabItems -->
                            <Border BorderBrush="{TemplateBinding BorderBrush}"
                            BorderThickness="{TemplateBinding BorderThickness}"
                            Background="{TemplateBinding Background}"
                            Grid.Column="1">
                                <ContentPresenter x:Name="PART_SelectedContentHost"
                                          ContentSource="SelectedContent"/>
                            </Border>
                        </Grid>
                    </ControlTemplate>
                </Setter.Value>
            </Setter>
            <Setter Property="ItemContainerStyle">
                <Setter.Value>
                    <Style TargetType="TabItem">
                        <Setter Property="Template">
                            <Setter.Value>
                                <ControlTemplate TargetType="TabItem">
                                    <Border Name="Border" BorderThickness="1,1,1,0" Background="{StaticResource TabColor}">
                                        <ContentPresenter ContentSource="Header" Margin="5"/>
                                    </Border>
                                    <ControlTemplate.Triggers>
                                        <Trigger Property="IsSelected" Value="True">
                                            <Setter TargetName="Border" Property="Background" Value="{StaticResource TabColorSelec}"/>
                                        </Trigger>
                                    </ControlTemplate.Triggers>
                                </ControlTemplate>
                            </Setter.Value>
                        </Setter>
                    </Style>
                </Setter.Value>
            </Setter>
        </Style>

        <Style TargetType="TabControl" x:Key="Custom1TabItemStyle">

            <Setter Property="ItemContainerStyle">
                <Setter.Value>
                    <Style TargetType="TabItem">
                        <Setter Property="Template">
                            <Setter.Value>
                                <ControlTemplate TargetType="TabItem">
                                    <Border Name="Border" BorderThickness="0" Background="#E8E8E8">
                                        <ContentPresenter ContentSource="Header" Margin="8 6"/>
                                    </Border>
                                    <ControlTemplate.Triggers>
                                        <Trigger Property="IsSelected" Value="True">
                                            <Setter TargetName="Border" Property="Background" Value="{StaticResource TabColorSelec}"/>
                                        </Trigger>
                                    </ControlTemplate.Triggers>
                                </ControlTemplate>
                            </Setter.Value>
                        </Setter>
                    </Style>
                </Setter.Value>
            </Setter>
        </Style>

        <Style x:Key="CustomProgressBarStyle" TargetType="ProgressBar">
            <Setter Property="Foreground" Value="#FFF" />
            <!-- Otros setters según tus necesidades -->
        </Style>

    </Window.Resources>

    <controls:MetroWindow.RightWindowCommands>
        <controls:WindowCommands>
            <Button Visibility="Hidden" x:Name="RegistroVersionBoton" Click="RegistrarVersion">
                <StackPanel Orientation="Horizontal">
                    <iconPacks:PackIconModern Width="20" Height="20" Kind="BookList" />
                    <TextBlock Margin="4 0 0 0" VerticalAlignment="Center" Text=" REGISTRAR VERSIÓN" />
                </StackPanel>
            </Button>
            <Button Visibility="Hidden" Click="cmdConfigurarBotonSuperior">
                <StackPanel Orientation="Horizontal">
                    <iconPacks:PackIconModern Width="20" Height="20" Kind="Cloud" />
                    <TextBlock Margin="4 0 0 0" VerticalAlignment="Center" Text=" ACTUALIZAR" />
                </StackPanel>
            </Button>
            <Button Visibility="Hidden">
                <Button.ContextMenu>
                    <ContextMenu>
                        <MenuItem Header="Cambiar Aspecto de la Interfaz"/>
                        <MenuItem Header="Restaurar Base de Datos"/>
                        <MenuItem Header="Respaldar Base de Datos"/>
                    </ContextMenu>
                </Button.ContextMenu>
                <StackPanel Orientation="Horizontal">
                    <iconPacks:PackIconModern Width="20" Height="20" Kind="Cog" />
                    <TextBlock Margin="4 0 0 0" VerticalAlignment="Center" Text=" HERRAMIENTAS" />
                </StackPanel>
            </Button>
            <Button Click="IrADistancia" Visibility="Hidden" >
                <StackPanel Orientation="Horizontal">
                    <iconPacks:PackIconModern Width="20" Height="20" Kind="MapTreasure" />
                    <TextBlock x:Name="Actividad_Trata" Margin="4 0 0 0" VerticalAlignment="Center" Text=" ACT 0 | PEND 0"/>
                </StackPanel>
            </Button>
            <Button Visibility="Hidden">
                <StackPanel Visibility="Collapsed" Orientation="Horizontal">
                    <iconPacks:PackIconModern Width="20" Height="20" Kind="Bug" />
                    <TextBlock Margin="4 0 0 0" VerticalAlignment="Center" Text=" REPORTAR FALLA"/>
                </StackPanel>
            </Button>
        </controls:WindowCommands>
    </controls:MetroWindow.RightWindowCommands>
